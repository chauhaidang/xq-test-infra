{
  "name": "xq-test-infra",
  "version": "0.0.2",
  "description": "A test infra cli used to spin up test environment",
  "main": "index.js",
  "bin": {
    "xq-infra": "./bin/xq-infra.js"
  },
  "scripts": {
    "start": "node ./bin/xq-infra.js",
    "test": "jest",
    "build": "find . -name '*.js' -not -path './node_modules/*' -print0 | xargs -0 node --check && npm test",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chauhaidang/xq-test-infra.git"
  },
  "keywords": [
    "cli"
  ],
  "author": "chauhaidang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/chauhaidang/xq-test-infra/issues"
  },
  "homepage": "https://github.com/chauhaidang/xq-test-infra#readme",
  "dependencies": {
    "@chauhaidang/xq-js-common-kit": "1.0.4",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "yaml": "^2.3.1",
    "cross-spawn": "7.0.6",
    "uuid": "^9.0.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "jest": "^30.1.1",
    "eslint": "^8.50.0"
  }
}
